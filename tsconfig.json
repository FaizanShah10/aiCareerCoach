{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "typeRoots": ["./types", "./node_modules/@types"],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/data/Features.js", "src/app/(auth)/sign-up/[[...sign-up]]/page.jsx", "src/app/(auth)/sign-in/[[...sign-in]]/page.jsx", "src/app/(main)/resume/components/form/PersonalInfo.jsx", "src/app/(main)/faizan/page.jsx", "src/app/(auth)/sign-in/page.jsx", "src/app/(auth)/sign-up/page.jsx", "src/app/(main)/cover-letter/page.tsx"],
  "exclude": ["node_modules"],
}
